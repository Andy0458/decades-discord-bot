<Configuration status="DEBUG">
    <Appenders>
        <Lambda name="Lambda">
            <JsonTemplateLayout eventTemplateUri="classpath:LogFormat.json" locationInfoEnabled="true"/>
        </Lambda>
    </Appenders>
    <Loggers>
        <Root level="DEBUG">
            <AppenderRef ref="Lambda"/>
        </Root>
    </Loggers>
</Configuration>
